{
    "author": "Coveo <sandbox_JSUI@coveo.com> (http://source.coveo.com/)",
    "license": "Apache-2.0",
    "name": "coveo-for-zendesk",
    "version": "0.0.1",
    "description": "Coveo UI Integration within Zendesk",
    "main": "./bin/commonjs/CoveoForZendesk.js",
    "module": "./bin/es6/Index.js",
    "types": "./bin/typings",
    "sideEffects": [
        "./bin/es6/components/InsightPanel/Strings.js"
    ],
    "scripts": {
        "test": "karma start ./karma.config.js --single-run",
        "testCoverage": "npm run test && cat ./bin/coverage/lcov.info | node ./node_modules/coveralls/bin/coveralls.js",
        "setup": "npm install",
        "build": "npm run clean && npm run build-es && npm run build-commonjs && npm run build-prod-commonjs",
        "build-es": "tsc -p .",
        "build-commonjs": "webpack --config ./webpack.config.js",
        "build-prod-commonjs": "webpack --config ./webpack.config.prod.js",
        "clean": "node ./scripts/setup.js",
        "dev": "node ./scripts/dev.server.js"
    },
    "pre-commit": [
        "lint-fix"
    ],
    "files": [
        "bin/commonjs/**/*",
        "bin/es6/**/*",
        "bin/css/**/*",
        "bin/img/**/*",
        "bin/typings/**/*",
        "src/**/*.scss"
    ],
    "keywords": [
        "coveo",
        "search",
        "ui",
        "zendesk",
        "js",
        "typescript",
        "jssearch",
        "jsui"
    ],
    "devDependencies": {
        "@cypress/webpack-preprocessor": "^4.1.2",
        "@types/chai": "^4.2.9",
        "@types/jasmine": "^3.3.9",
        "@types/mocha": "^7.0.1",
        "@types/node": "^11.11.3",
        "@types/sinon": "^7.0.13",
        "coveo-search-ui": "2.8521.7",
        "coveo-search-ui-extensions": "^0.0.29",
        "coveo-search-ui-tests": "0.0.7",
        "coveralls": "^3.0.3",
        "cypress": "^4.0.2",
        "istanbul-instrumenter-loader": "^3.0.1",
        "jasmine": "^3.3.1",
        "karma": "^4.4.1",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage-istanbul-reporter": "^2.0.5",
        "karma-jasmine": "^2.0.1",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "0.0.32",
        "karma-webpack": "^3.0.5",
        "ncp": "^2.0.0",
        "node-sass": "^4.13.1",
        "path": "^0.12.7",
        "pre-commit": "^1.2.2",
        "prettier": "1.16.4",
        "puppeteer": "^2.0.0",
        "raw-loader": "^1.0.0",
        "rimraf": "^2.6.3",
        "sinon": "^7.3.2",
        "snyk": "^1.230.4",
        "string-replace-webpack-plugin": "^0.1.3",
        "ts-es5-istanbul-coverage": "^1.0.6",
        "ts-loader": "^6.0.0",
        "typescript": "^3.3.0",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.11.0",
        "webpack-karma-die-hard": "^1.0.4",
        "xml2js": "^0.4.23"
    },
    "snyk": true,
    "dependencies": {
        "@coveord/platform-client": "^15.0.0"
    }
}
